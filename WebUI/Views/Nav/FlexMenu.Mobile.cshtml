@model IEnumerable<string>
 <div data-role="navbar">
     <ul>
         @foreach (var link in Model)
         {
             @Html.RouteLink(link, new
        {
            controller = "Product",
            action = "List",
            category = link,
            page = 1
        }, new { data_transition = "fade", @class = (link == ViewBag.SelectedCategory ? "ui-btn-active" : null) })}
     </ul>
</div>
